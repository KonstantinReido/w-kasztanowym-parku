{
  "manifest_version": 2,
  "name": "W Kasztanowym Parku Photo Downloader",
  "version": "1.0",
  "description": "Download current photo in a photo gallery from the www.wkasztanowymparku.pl",
  "permissions": [
    "activeTab"
  ],
  "background": {
      "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon" : "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128":"images/icon.png"
  }
}
